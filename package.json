    "laravel-mix": "^4.0.15",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.20.1",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "css-loader": "^2.1.1",
    "material-design-icons-iconfont": "^5.0.1",
    "vuetify": "^1.5.14",
    "tar": "^4.4.8"
